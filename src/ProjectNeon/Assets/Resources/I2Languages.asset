        \u043D\u0430\u0440\u043A\u043E\u0442\u0438\u043A\u0430\u043C, \u043A\u043E\u0442\u043E\u0440\u044B\u0435
        \u043C\u043E\u0433\u0443\u0442 \u0438\u043C\u0435\u0442\u044C \u043F\u043E\u0431\u043E\u0447\u043D\u044B\u0439
        \u044D\u0444\u0444\u0435\u043A\u0442 \u043E\u0442 \u0443\u0431\u0438\u0439\u0441\u0442\u0432\u0430
