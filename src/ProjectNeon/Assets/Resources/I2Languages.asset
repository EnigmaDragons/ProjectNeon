      - "<b>{[t:Keywords/Swap]}</b>\n\n\u3053\u306E\u30AB\u30FC\u30C9\u304C\u89E3\u6C7A\u3057\u305F\u5F8C\u3001\u6307\u5B9A\u3055\u308C\u305F\u30AB\u30FC\u30C9\u3068\u4EA4\u63DB\u3057\u307E\u3059"
