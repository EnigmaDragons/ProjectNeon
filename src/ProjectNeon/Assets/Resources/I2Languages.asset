        \u0434\u0438\u0437\u0430\u0439\u043D\u0430.\n\n\u0423\u043D\u0438\u0432\u0435\u0440\u0441\u0430\u0442\u0435\u043B\u0438
        {[t:MegaCorps/Plasmigarchy]} \u0443\u0442\u0432\u0435\u0440\u0436\u0434\u0430\u044E\u0442,
        \u0447\u0442\u043E \u0435\u0434\u0438\u043D\u0441\u0442\u0432\u0435\u043D\u043D\u0430\u044F
        \u043F\u0440\u0438\u0447\u0438\u043D\u0430, \u043F\u043E \u043A\u043E\u0442\u043E\u0440\u043E\u0439
