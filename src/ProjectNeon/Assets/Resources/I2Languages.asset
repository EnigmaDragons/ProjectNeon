        \u0441\u043E\u0437\u043D\u0430\u043D\u0438\u0435, Medigeneix \u0432\u044B\u0437\u044B\u0432\u0430\u0435\u0442
        \u0431\u0440\u0438\u0433\u0430\u0434\u0443 \u043C\u0435\u0434\u0438\u043A\u043E\u0432,
        \u0447\u0442\u043E\u0431\u044B \u043F\u043E\u0433\u0440\u0443\u0437\u0438\u0442\u044C
        \u0432\u0430\u0448 \u043E\u0442\u0440\u044F\u0434 \u0432 MedEvac. \u041C\u0435\u0434\u0438\u0446\u0438\u043D\u0441\u043A\u0430\u044F
        \u0431\u0440\u0438\u0433\u0430\u0434\u0430 \u043E\u0441\u0442\u043E\u0440\u043E\u0436\u043D\u043E
        \u0437\u0430\u0433\u0440\u0443\u0436\u0430\u0435\u0442 \u043A\u0430\u0436\u0434\u043E\u0435
        \u0442\u0435\u043B\u043E \u0432 \u043F\u0430\u0440\u044F\u0449\u0443\u044E
        \u043C\u0430\u0448\u0438\u043D\u0443. \u0411\u0435\u0441\u0441\u0442\u0440\u0430\u0441\u0442\u043D\u043E
        \u043A\u043E\u043C\u0430\u043D\u0434\u0430 \u043E\u0441\u0442\u043E\u0440\u043E\u0436\u043D\u043E
        \u0432\u0432\u043E\u0434\u0438\u0442 \u0441\u043C\u0435\u0440\u0442\u0435\u043B\u044C\u043D\u0443\u044E
