        \u0412 \u043C\u0438\u0440\u0435 \u0431\u0435\u0437 \u0433\u0440\u0430\u0436\u0434\u0430\u043D\u0441\u043A\u043E\u0439
        \u043F\u043E\u043B\u0438\u0446\u0438\u0438 \u043B\u044E\u0431\u043E\u0433\u043E,
        \u043A\u0442\u043E \u0445\u043E\u0447\u0435\u0442, \u0447\u0442\u043E\u0431\u044B
        \u0443\u0441\u043B\u0443\u0433\u0438 \u043F\u043E \u0437\u0430\u0449\u0438\u0442\u0435
        \u0438\u043B\u0438 \u043A\u0440\u0438\u043C\u0438\u043D\u0430\u043B\u044C\u043D\u043E\u043C\u0443
        \u0440\u0435\u0448\u0435\u043D\u0438\u044E \u0434\u043E\u043B\u0436\u043D\u044B
        \u043F\u043B\u0430\u0442\u0438\u0442\u044C \u0437\u0430 \u044D\u0442\u043E.
