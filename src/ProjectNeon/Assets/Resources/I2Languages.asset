      - 
      - "\u0413\u043B\u0430\u0432\u043D\u044B\u0439 \u0432 \u043C\u0438\u0440\u0435
        \u043F\u0440\u043E\u0438\u0437\u0432\u043E\u0434\u0438\u0442\u0435\u043B\u044C
        \u0440\u043E\u0431\u043E\u0442\u0438\u0437\u0438\u0440\u043E\u0432\u0430\u043D\u043D\u044B\u0445
        \u0443\u0441\u0442\u0440\u043E\u0439\u0441\u0442\u0432 \u0431\u0435\u0437\u043E\u043F\u0430\u0441\u043D\u043E\u0441\u0442\u0438
        \u0432\u0441\u0435\u0445 \u0432\u0438\u0434\u043E\u0432.\n\n\u0418\u0441\u043F\u043E\u043B\u044C\u0437\u0443\u044F
        \u043F\u0440\u043E\u0434\u0443\u043A\u0442\u044B {[t:MegaCorps/Secubotics]},
        \u0432\u044B \u043C\u043E\u0436\u0435\u0442\u0435 \u043F\u043E\u043B\u043D\u043E\u0441\u0442\u044C\u044E
        \u0430\u0432\u0442\u043E\u043C\u0430\u0442\u0438\u0437\u0438\u0440\u043E\u0432\u0430\u0442\u044C
        \u0441\u0432\u043E\u044E \u0431\u0438\u0437\u043D\u0435\u0441-\u0431\u0435\u0437\u043E\u043F\u0430\u0441\u043D\u043E\u0441\u0442\u044C
        \u0441 \u043F\u043E\u043C\u043E\u0449\u044C\u044E \u043D\u0435\u043F\u0440\u0435\u043E\u0434\u043E\u043B\u0438\u043C\u044B\u0445
        \u0440\u043E\u0431\u043E\u0442\u043E\u0432 \u0434\u043B\u044F \u0437\u0430\u0449\u0438\u0442\u044B
        \u043F\u0440\u043E\u043C\u044B\u0448\u043B\u0435\u043D\u043D\u043E\u0433\u043E
        \u0443\u0440\u043E\u0432\u043D\u044F \u0432\u0441\u0435\u0445 \u0444\u043E\u0440\u043C
        \u0438 \u0440\u0430\u0437\u043C\u0435\u0440\u043E\u0432, \u0434\u043E\u0441\u0442\u0443\u043F\u043D\u044B\u0445
        \u0434\u043B\u044F \u0432\u0441\u0435\u0445 \u0441\u0440\u0435\u0434, \u043E\u0442
        \u0433\u043E\u0440\u043E\u0434\u0441\u043A\u0438\u0445 \u0443\u0441\u043B\u043E\u0432\u0438\u0439
        \u0434\u043E \u0434\u0436\u0443\u043D\u0433\u043B\u0435\u0439 \u0438\u043B\u0438
        \u0434\u0430\u0436\u0435 \u043F\u043E\u0434 \u0432\u043E\u0434\u043E\u0439.
        {[t:MegaCorps/Secubotics]} \u043F\u0440\u0435\u0434\u043E\u0441\u0442\u0430\u0432\u043B\u044F\u0435\u0442
        \u0441\u043E\u043E\u0431\u0449\u0435\u0441\u0442\u0432\u0443 \u043B\u0443\u0447\u0448\u0438\u0435
        \u0432 \u0432\u044B\u0441\u043E\u043A\u043E\u0442\u0435\u0445\u043D\u043E\u043B\u043E\u0433\u0438\u0447\u043D\u043E\u0439
