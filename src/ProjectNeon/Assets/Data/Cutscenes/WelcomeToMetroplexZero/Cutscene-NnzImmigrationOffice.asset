      Variable: {fileID: 0}
  - Id: 11
    SegmentType: 1
    DialogueCharacterId:
      UseConstant: 1
      ConstantValue: Trainer
      Variable: {fileID: 0}
    Text: 'None of the MetroCorps compete with each other. We all work together for
