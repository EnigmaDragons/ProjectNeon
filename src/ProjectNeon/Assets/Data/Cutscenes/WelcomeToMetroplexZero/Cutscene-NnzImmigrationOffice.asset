      Variable: {fileID: 0}
  - Id: 10
    SegmentType: 1
    DialogueCharacterId:
      UseConstant: 1
      ConstantValue: Trainer
      Variable: {fileID: 0}
    Text: "I'll be honest. We want you here for your incredible talent stack. You're
      smart, technogically capable, and have incredible spiritual focus. \n\nAs per
      the ICC guidance, we are completely happy no matter which corporation you join
      as an affiliated benefactor. "
    FloatAmount:
      UseConstant: 0
      ConstantValue: 0
      FloatVariable: {fileID: 0}
    CutsceneEventName:
      UseConstant: 1
      ConstantValue: 
      Variable: {fileID: 0}
    StoryEvent: {fileID: 0}
