    FloatAmount:
      UseConstant: 1
      ConstantValue: 0
      FloatVariable: {fileID: 0}
    StoryEvent: {fileID: 0}
  - SegmentType: 1
    DialogueCharacterId:
      UseConstant: 0
      ConstantValue: 1
      Variable: {fileID: 11400000, guid: 0f34406efeaef1547a262632b07391ed, type: 2}
