  - SegmentType: 1
    DialogueCharacterId:
      UseConstant: 0
      ConstantValue: 
      Variable: {fileID: 11400000, guid: e4448d6f7fbaa0141a8c686e37c976a3, type: 2}
    Text: I have been making a virus to kill or control them.
    FloatAmount:
      UseConstant: 1
      ConstantValue: 0
      FloatVariable: {fileID: 0}
    StoryEvent: {fileID: 0}
    RequiredStates: []
    ForbiddenStates:
