    Or: 0
    StoryState:
      UseConstant: 1
      ConstantValue: 
      Variable: {fileID: 0}
  - SegmentType: 1
    DialogueCharacterId:
      UseConstant: 0
      ConstantValue: 
      Variable: {fileID: 11400000, guid: 0f34406efeaef1547a262632b07391ed, type: 2}
    Text: "How do we know you\u2019re telling the truth and aren\u2019t full of shit?"
    FloatAmount:
      UseConstant: 1
      ConstantValue: 0
      FloatVariable: {fileID: 0}
    StoryEvent: {fileID: 0}
    RequiredStates:
    - UseConstant: 0
      ConstantValue: 
      Variable: {fileID: 11400000, guid: 7da1bc35fdf6a5a4b8ccd52661ca2119, type: 2}
    ForbiddenStates: []
    Or: 0
    StoryState:
      UseConstant: 1
      ConstantValue: 
      Variable: {fileID: 0}
  - SegmentType: 1
    DialogueCharacterId:
      UseConstant: 0
      ConstantValue: 
      Variable: {fileID: 11400000, guid: e4448d6f7fbaa0141a8c686e37c976a3, type: 2}
    Text: I am unofficially the head of secubotics intelligence. You can check the
      files on my computer.
    FloatAmount:
      UseConstant: 1
      ConstantValue: 0
      FloatVariable: {fileID: 0}
    StoryEvent: {fileID: 0}
    RequiredStates:
    - UseConstant: 0
      ConstantValue: 
      Variable: {fileID: 11400000, guid: 7da1bc35fdf6a5a4b8ccd52661ca2119, type: 2}
