      ConstantValue: 
      Variable: {fileID: 0}
  - SegmentType: 1
    DialogueCharacterId:
      UseConstant: 0
      ConstantValue: 
      Variable: {fileID: 11400000, guid: 0f34406efeaef1547a262632b07391ed, type: 2}
